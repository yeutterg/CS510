
public class Move {
	
	// Constructor for a move for a specific piece
	
	public static int pieceNum; 
	public static char moveId;
	
	public Move(int currentPieceNum, char currentMove) {
		pieceNum = currentPieceNum;
		moveId = currentMove;
	}
	
	Move(Move m) {
		pieceNum = m.pieceNum;
		moveId = m.moveId;
	}
	
	public int getPieceNum() {
		return pieceNum;
	}
	
	public char getMove() {
		return moveId;
	}
	
	

}
